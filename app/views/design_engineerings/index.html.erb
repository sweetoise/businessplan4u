<section class="content-header">
	<h1> Design & Engineering Services <small>Design & Engineering Services table</small></h1>
	<ol class="breadcrumb">
		<li>
			<a><i class="fa fa-dashboard"></i> Home</a>
		</li>
		<li class="active">
			<a>Design & Engineering Service</a>
		</li>
	</ol>
</section>

<!-- Main content -->
<section class="content">
	<div class="row">
		<div class="col-xs-12">
			<div class="box">
				<div class="box-header">
					<h3 class="box-title">Design & Engineering Service Data With Full Features</h3>
				</div><!-- /.box-header -->
				<div class="box-body">
					<table id="example1" class="table table-bordered table-striped">
						<thead>
							<tr>
								<th>Title</th>
								<th>Description</th>
								<th>Show content</th>
								<th>Edit content</th>
								<th>Delete content</th>
							</tr>
						</thead>
						<tbody>
							<% @design_engineerings.each do |design_engineering| %>
							<tr>
								<td><%= design_engineering.title %></td>
								<td><%= truncate design_engineering.description, length: 75 %></td>
								<td><%= link_to 'Show', design_engineering %></td>
								<td><%= link_to 'Edit', edit_design_engineering_path(design_engineering) %></td>
								<td><%= link_to 'Destroy', design_engineering, method: :delete, data: { confirm: 'Are you sure?' } %></td>
							</tr>
							<% end %>
						</tbody>
						<tfoot>
							<tr>
								<th>Title</th>
								<th>Description</th>
								<th>Show content</th>
								<th>Edit content</th>
								<th>Delete content</th>
							</tr>
						</tfoot>
					</table>
				</div><!-- /.box-body -->
			</div><!-- /.box -->
		</div><!-- /.col -->
	</div><!-- /.row -->

	<!-- Default box -->
	<div class="box">
		<div class="box-footer">
			<!-- Block buttons -->
			<div class="box-header">
				<h3 class="box-title">New Design & Engineering Service</h3>
			</div>
			<div class="box-body">
				<div class="add-inst">
					<%= link_to '+  ADD DETAILS', new_design_engineering_path, :class => 'btn btn-primary btn-block' %>
				</div>
			</div>
		</div><!-- /.box-footer-->
	</div><!-- /.box -->
</section>

