<section class="content-header">
	<h1> Engineering & Project Mgmts <small>Engineering & Project Mgmts table</small></h1>
	<ol class="breadcrumb">
		<li>
			<a><i class="fa fa-dashboard"></i> Home</a>
		</li>
		<li class="active">
			<a>Engineering & Project Mgmt</a>
		</li>
	</ol>
</section>

<!-- Main content -->
<section class="content">
	<div class="row">
		<div class="col-xs-12">
			<div class="box">
				<div class="box-header">
					<h3 class="box-title">Engineering & Project Mgmt Data With Full Features</h3>
				</div><!-- /.box-header -->
				<div class="box-body">
					<table id="example1" class="table table-bordered table-striped">
						<thead>
							<tr>
								<th>Title</th>
								<th>Description</th>
								<th>Show content</th>
								<th>Edit content</th>
								<th>Delete content</th>
							</tr>
						</thead>
						<tbody>
							<% @engineering_mgmts.each do |engineering_mgmt| %>
							<tr>
								<td><%= engineering_mgmt.title %></td>
								<td><%= truncate engineering_mgmt.description, length: 75 %></td>
								<td><%= link_to 'Show', engineering_mgmt %></td>
								<td><%= link_to 'Edit', edit_engineering_mgmt_path(engineering_mgmt) %></td>
								<td><%= link_to 'Destroy', engineering_mgmt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
							</tr>
							<% end %>
						</tbody>
						<tfoot>
							<tr>
								<th>Title</th>
								<th>Description</th>
								<th>Show content</th>
								<th>Edit content</th>
								<th>Delete content</th>
							</tr>
						</tfoot>
					</table>
				</div><!-- /.box-body -->
			</div><!-- /.box -->
		</div><!-- /.col -->
	</div><!-- /.row -->

	<!-- Default box -->
	<div class="box">
		<div class="box-footer">
			<!-- Block buttons -->
			<div class="box-header">
				<h3 class="box-title">New Engineering & Project Mgmt Detail</h3>
			</div>
			<div class="box-body">
				<div class="add-inst">
					<%= link_to '+  ADD DETAILS', new_engineering_mgmt_path, :class => 'btn btn-primary btn-block' %>
				</div>
			</div>
		</div><!-- /.box-footer-->
	</div><!-- /.box -->
</section>

